<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>cyberArt</title> 
    <!-- add icon to url -->
</head>
<body>
    <h1>"Hello Its Andre"</h1>
    <a href="/arts">Back To Index Page (Home Page)</a>
    <ul>
        <% Art.forEach(Art => { %>
                <a href="/arts/<%= Art._id; %>"> </br>
                <form  action="/arts/<%= Art.id %>?_method=DELETE" method="POST" style="display:inline; margin-left:16px;">
                    <input type="submit" value="DELETE" />
                </form>
                <a href="/arts/<%= Art._id; %>/edit">Edit Art</a>
               <% }) %>    
      </ul>
   
    <nav>
        <a href="/arts/new">Create A New Art</a>
    </nav>

</body>
</html>